                                                            <label class="col-xl-3 col-lg-3 text-end mb-lg-0 align-self-center">BVN</label>
                                                            <div class="col-lg-9 col-xl-8">
                                                                <input class="form-control custom-input" style="pointer-events: none;" type="text" name='BVN' value="<% if (data.userID) { %>
                                                                    <%=data.userID.BVN %> 
                                                                   <% } else { %>
                                                                       <%="" %> 
                                                                   <% } %> " required  >
                                                            </div>
                                                        </div>
                                                        <div class="form-group row">
                                                            <label class="col-xl-3 col-lg-3 text-end mb-lg-0 align-self-center">NIN</label>
                                                            <div class="col-lg-9 col-xl-8">
                                                                <input class="form-control custom-input" style="pointer-events: none;" name="NIN" type="text" value="<% if (data.userID) { %>
                                                                 <%=data.userID.NIN %> 
                                                                <% } else { %>
                                                                    <%="" %> 
                                                                <% } %> " required  >
                                                            </div>
                                                        </div>
                                                        <div style="float: right;" class="px-5">
                                                            <button disabled=<% if (data.userID) { %>
                                                                <%=true %> 
                                                             
                                                            <% } else { %>
                                                                <%=false %> 
                                                            <% } %>  class="btn btn-success btn-sm">VALIDATE</button>
                                                        </div>
                                                    </form>

                                                    
                                                </div><!--end card-body-->
                                            </div><!--end card-->
                                            
                                        </div> <!-- end col -->                                                                              
                                    </div><!--end row-->
                                </div><!--end tab-pane-->
                            </div><!--end tab-content-->
                        </div><!--end col-->
                    </div><!--end row-->

                </div><!-- container -->

                 <footer class="footer text-center text-sm-start">
                    &copy; <script>
                        document.write(new Date().getFullYear())
                    </script> Verido 
                </footer><!--end footer-->
            </div>           

        


        <!-- jQuery  -->
