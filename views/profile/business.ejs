        <!-- App js -->
        <script src="/assets/js/app.js"></script>
        <script>
            const doc = Array.from(document.querySelectorAll('.custom-input'))
            doc.forEach(element => {
                element.onkeydown = (event) => (
                        event.preventDefault()
                )})
            doc.oninput = function(event){
                event.preventDefault()
            }
        </script>
        
